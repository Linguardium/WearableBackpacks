{
  "schemaVersion": 1,
  "id": "wearablebackpacks",
  "name": "Wearable Backpacks",
  "version": "$version",
  "description": "Adds beautiful, balanced backpacks",
  "license": "MIT",
  "icon": "assets/wearablebackpacks/icon.png",
  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric-api": ">=0.92.1",
    "minecraft": "1.20.1",
    "java": ">=17"
  },
  "entrypoints": {
    "client": [
      "dev.sapphic.wearablebackpacks.client.BackpacksClient"
    ],
    "main": [
      "dev.sapphic.wearablebackpacks.Backpacks",
      "dev.sapphic.wearablebackpacks.advancement.BackpackCriteria",
      "dev.sapphic.wearablebackpacks.network.BackpackServerNetwork",
      "dev.sapphic.wearablebackpacks.stat.BackpackStats"
    ],
    "modmenu": [
      "dev.sapphic.wearablebackpacks.client.integrations.ModMenuIntegration"
    ]
  },
  "mixins": [
    "wearablebackpacks.mixins.json",
    {
      "config": "wearablebackpacks.client.mixins.json",
      "environment": "client"
    }
  ],
  "authors": [
    "Chloe Dawn",
